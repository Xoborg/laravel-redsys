{
    "name": "xoborg/laravel-redsys",
    "description": "Package para usar la pasarela de pago Redsys en Laravel",
    "keywords": [
        "xoborg",
        "redsys",
        "tpv",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Xoborg",
            "email": "developers@xoborg.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Xoborg\\LaravelRedsys\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xoborg\\LaravelRedsys\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xoborg\\LaravelRedsys\\LaravelRedsysServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
